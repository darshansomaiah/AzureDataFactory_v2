{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf2ccd"
		},
		"AzureBlobStorage_Destination_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_Destination'"
		},
		"AzureBlobStorage_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_LS'"
		},
		"AzureBlobStorage_Source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_Source'"
		},
		"AzureDataLakeStorage_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_LS'"
		},
		"AzureSqlDatabase_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase_LS'"
		},
		"LS_AzureSqlDatabase_1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureSqlDatabase_1'"
		},
		"LS_AzureSqlDatabase_2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureSqlDatabase_2'"
		},
		"AzureDataLakeStorage_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dl2ccd.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf2ccd_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_Destination_connectionString": {
						"value": "[parameters('AzureBlobStorage_Destination_connectionString')]"
					},
					"AzureBlobStorage_LS_connectionString": {
						"value": "[parameters('AzureBlobStorage_LS_connectionString')]"
					},
					"AzureBlobStorage_Source_connectionString": {
						"value": "[parameters('AzureBlobStorage_Source_connectionString')]"
					},
					"AzureDataLakeStorage_LS_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_LS_accountKey')]"
					},
					"AzureSqlDatabase_LS_connectionString": {
						"value": "[parameters('AzureSqlDatabase_LS_connectionString')]"
					},
					"LS_AzureSqlDatabase_1_connectionString": {
						"value": "[parameters('LS_AzureSqlDatabase_1_connectionString')]"
					},
					"LS_AzureSqlDatabase_2_connectionString": {
						"value": "[parameters('LS_AzureSqlDatabase_2_connectionString')]"
					},
					"AzureDataLakeStorage_LS_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_LS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf2ccd_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf2ccd_ArmTemplate_0')]"
			]
		}
	]
}